@model IEnumerable<MovieJournalDTO.MovieOnList>

@foreach (var item in Model)
{
    @TMDBGateway.TMDBServices.TMDBGateWayService.Instance.GetMovie(item.MovieId).Title
    <h2>@Html.DisplayFor(modelItem => item.Profile)</h2>
    <h2>@Html.DisplayFor(modelItem => item.Rating)</h2>
    <h2>@Html.DisplayFor(modelItem => item.Review)</h2>
}